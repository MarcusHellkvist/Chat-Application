<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="35dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#E3AB9B"
        android:startColor="#CFBDB6"
        android:endColor="#764436"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />
    <size
        android:width="135dp"
        android:height="50dp"
        />
    <stroke
        android:width="3dp"
        android:color="#878787"
        />
</shape>