<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
    <corners
        android:radius="35dp"
        />
    <gradient
        android:angle="45"
        android:centerX="35%"
        android:centerColor="#F4F5F6"
        android:startColor="@color/colorWhiteButton"
        android:endColor="@color/colorWhiteButton"
        android:type="linear"
        />
    <padding
        android:left="10dp"
        android:top="10dp"
        android:right="10dp"
        android:bottom="10dp"
        />
    <size
        android:width="235dp"
        android:height="60dp"
        />
    <stroke
        android:width="0dp"
        android:color="@color/colorButtonAccent"
        />
</shape>